{
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "koverage.coverageFileNames": [
        "lcov.info",
        "cov.xml",
        "coverage.xml",
        "jacoco.xml"
    ],
    "koverage.coverageFilePaths": [
        "."
    ],
    "koverage.lowCoverageThreshold": 50,
    "koverage.sufficientCoverageThreshold": 75,
    "coverage-gutters.showGutterCoverage": false,
    "coverage-gutters.showLineCoverage": true,
    "private-extension-manager.directories": [
        ".extensions"
    ],
    "private-extension-manager.autoUpdate": true
}